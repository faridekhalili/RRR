{
  "name": "Rerendering Profiler Extension",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/dist/react.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}